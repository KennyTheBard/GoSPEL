
let
    (load test.jpg)
    img
    (save
        (resize
            $img
            (let
                (sizeof $img)
                size
                (rect new
                    (rect first $size)
                    (point add
                        (rect last $size)
                        (point new -250 750)
                    )
                )
            )
        )
        result
        jpg
    )
